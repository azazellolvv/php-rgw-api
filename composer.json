{
  "name": "myena/php-rgw-api",
  "description": "PHP Client for the Rados Gateway API that doesn't suck",
  "type": "library",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "Daniel Carbone",
      "email": "dcarbone@ena.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "6.3.*",
    "psr/log": "1.*",
    "dcarbone/gohttp": "0.1.*"
  },
  "require-dev": {
    "myena/default-logger": "@stable",
    "zircote/swagger-php": "@stable"
  },
  "autoload": {
    "psr-4": {
      "MyENA\\RGW\\": "src/"
    },
    "files": [
      "files/constants.php",
      "files/funcs.php"
    ]
  },
  "suggest": {
    "zircote/swagger-php": "Swagger docs!",
    "myena/default-logger": "Simple PSR-compliant logger with ANSI color support"
  }
}